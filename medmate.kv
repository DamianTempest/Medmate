<NxtScreen>:
    name: "nxt"
    BoxLayout:
        orientation: "vertical"
        spacing: dp(10)
        padding: dp(20)
        canvas.before:
            Rectangle:
                source: "a.jpg"
                pos: self.pos
                size: self.size 

        Label:
            text: "Edit Patient Screen"
            font_size: sp(24)
            bold: True
            halign: "center"
            valign: "middle"
            text_size: (self.width, None)
            size_hint_y: None
            height: self.texture_size[1] + dp(10)

        ScrollView:
            do_scroll_x: False
            do_scroll_y: True

            BoxLayout:
                orientation: "vertical"
                size_hint_y: None
                height: self.minimum_height
                spacing: dp(10)

                TextInput:
                    id: name_input
                    hint_text: "Name"
                    multiline: False
                    size_hint_y: None
                    height: dp(50)
                    background_color: 1, 1, 1, 1
                    foreground_color: 0, 0, 0, 1
               
                TextInput:
                    id: age_input
                    hint_text: "Your Age"
                    multiline: False
                    size_hint_y: None
                    height: dp(50)
                    input_filter: "int"
                    background_color: 1, 1, 1, 1
                    foreground_color: 0, 0, 0, 1

                                

                Spinner:
                    id: preference_spinner
                    text: "Select Country"
                    values: ["Option 1", "Option 2", "Option 3"]
                    size_hint_y: None
                    height: dp(50)
                    background_color: 0.8, 0.8, 0.8, 1
                    color: 0, 0, 0, 1


                Label:
                    id: error_label
                    text: ""
                    color: 1, 0, 0, 1  # Red text for error messages
                    halign: "center"
                    valign: "middle"
                    text_size: (self.width, None)
                    size_hint_y: None
                    height: dp(30)

                BoxLayout:
                    orientation: "horizontal"
                    spacing: dp(10)
                    size_hint_y: None
                    height: dp(50)

                    Button:
                        text: "Back"
                        size_hint_x: 0.5
                        size_hint_y: None
                        height: dp(50)
                        background_color: 0.96, 0.26, 0.21, 1
                        background_normal: ''
                        color: 1, 1, 1, 1
                        on_release: app.root.current = "signup" 

                    Button:
                        text: "Signup"
                        size_hint_x: 0.5
                        size_hint_y: None
                        height: dp(50)
                        background_color: 0.13, 0.6, 0.95, 1
                        background_normal: ''
                        color: 1, 1, 1, 1
                        on_press: root.validate_Nxt()




                      
        

