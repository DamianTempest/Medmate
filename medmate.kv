<ai_chatapp>:
    name: 'ai_chatapp'

    BoxLayout:
        orientation: 'vertical'
        padding: 40
        spacing: 20

        # App logo/icon
        Image:
            source: "Mage.jpg"  
            size_hint: None, None
            size: 100, 100
            pos_hint: {"center_x": 0.5}

        # Welcome label
        Label:
            text: "Healthcare Assistant"
            font_size: "32sp"
            size_hint_y: 0.2
            color: 0.13, 0.6, 0.95, 1  # Color for #2196F3
            bold: True

        # Buttons layout
        BoxLayout:
            orientation: 'vertical'
            spacing: 25
            size_hint_y: 0.5

            Button:
                text: "My Profile"
                size_hint_y: None
                height: "70dp"
                background_color: 0.13, 0.6, 0.95, 1  # Color for #2196F3
                background_normal: ""
                font_size: "18sp"
                bold: True
                on_press: root.switch_screen("profile")

            Button:
                text: "AI Health Assistant"
                size_hint_y: None
                height: "70dp"
                background_color: 0.29, 0.69, 0.31, 1  # Color for #4CAF50
                background_normal: ""
                font_size: "18sp"
                bold: True
                on_press: root.send_chat_message("Hello, AI Health Assistant!")

 


        
