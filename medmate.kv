<PatientProfileScreen>:
    name: "patientprofile"
    BoxLayout:
        orientation: "vertical"
        
        canvas.before:
            Rectangle:
                source: "background.jpg"  # Your background image file
                pos: self.pos
                size: self.size
	
        BoxLayout:
            orientation: "horizontal"
            size_hint: (1, 0.3)
            padding: [dp(10), dp(10)]  # Fixing the padding issue
            spacing: dp(10)

            Image:
                source: "pt.jpg"  # Your profile picture file
                size_hint: (0.4, 1)
                allow_stretch: True

            BoxLayout:
                orientation: "vertical"
                size_hint: (0.6, 1)
                Label:
                    text: "Name:" + root.patient_name
                    font_size: sp(24)
                    bold: True
                    color: 0, 0, 0, 1
                Label:
                    text: "Status message goes here"
                    font_size: sp(16)
                    color: 0, 0, 0, 0.7
                Label:
                    text: "Age:" + root.patient_age
                    font_size: sp(24)
                    bold: True
                    color: 0, 0, 0, 1
                

        BoxLayout:
            orientation: "horizontal"
            size_hint: (1, 0.1)
            padding: [dp(10), dp(10)]  # Fixing the padding issue
            spacing: dp(10)

            Button:
                text: "Edit Profile"
                size_hint: (0.5, 1)
                background_normal: ""
                background_color: 0.25, 0.41, 0.88, 1
                color: 1, 1, 1, 1
                on_press: app.root.current ="editpatient"

            Button:
                text: "Settings"
                size_hint: (0.5, 1)
                background_normal: ""
                background_color: 0.25, 0.41, 0.88, 1
                color: 1, 1, 1, 1

            Button:
                text: "Back"
                size_hint: (0.5, 1)
                background_normal: ""
                background_color: 0.25, 0.41, 0.88, 1
                color: 1, 1, 1, 1
                on_press: app.root.current = "home"

        Button:
            text: "About"
            font_size: sp(20)
            color: 0, 0, 0, 1
            size_hint_y: None
            height: dp(30)
            padding: [dp(10), dp(10)]  
            bold: True
            on_press: app.root.current = "About"


        Label:
            text: "This is where the user's bio or additional information will be displayed. It can be a multiline text and provides more details about the user."
            font_size: sp(16)
            color: 0, 0, 0, 0.7
            size_hint_y: None
            height: dp(100)
            padding: [dp(10), dp(10)]  
            text_size: self.width - dp(20), None
            halign: "left"
            valign: "top"
        

<EditPatientScreen>:
