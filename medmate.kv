<PageScreen>:
    name: "page"
    canvas.before:
        Rectangle:
            source: "a.jpg"
            pos: self.pos
            size: self.size  
    BoxLayout:
        orientation: "vertical"
        spacing: dp(20)
        padding: dp(20)                 
        Label:
            color: 0, 0, 0, 1
            text: "Welcome To MedMate"
            font_size: dp(30)
            bold: True
            underline: True
            
        BoxLayout:
            orientation: "horizontal"
            spacing: dp(20)
            padding: dp(20)
            Button:
                background_color: 1, 1, 1, 0.5
                text: "Patient"
                size_hint: 0.3, 0.3
                pos_hint: {"center_y": 0.5}
                bold: True
                on_press: app.root.current = "signup"
            Button:
                background_color: 1, 1, 1, 0.5
                text: "Doctor"
                size_hint: 0.3, 0.3
                pos_hint: {"center_y": 0.5}
                on_press: app.root.current = "signup.doc"
            BoxLayout:
                orientation: "horizontal"
                spacing: dp(20)
                padding: dp(20)    
                Label:
                    text: "Choose Category"
                    font_size: sp(24)
                    pos_hint: {"center_y": 0.5}
                    bold: True
                    color: 0.25, 0.88, 0.82, 1   


<LoginScreen>:
    name: "login"
    BoxLayout:
        orientation: "vertical"
        padding: dp(20)
        spacing: dp(10)
        canvas.before:
            Rectangle:
                source: "a.jpg"
                pos: self.pos
                size: self.size 
        Label:
            text: "Login"
            font_size: dp(24)
            bold: True
            underline: True
            color: 0, 0, 0, 1

        TextInput:
            id: email
            hint_text: "Email"
            multiline: False
            size_hint_y: None
            height: dp(50)
            background_color: 1, 1, 1, 1
            foreground_color: 0, 0, 0, 1

        TextInput:
            id: password
            hint_text: "Password"
            multiline: False
            password: True
            size_hint_y: None
            height: dp(50)
            background_color: 1, 1, 1, 1
            foreground_color: 0, 0, 0, 1

        Label:
            id: error_label
            text: ""
            color: 1, 0, 0, 1  # Red for errors

        Button:
            text: "Login"
            size_hint_y: None
            height: dp(50)
            background_color: 0.13, 0.6, 0.95, 1
            background_normal: ''
            color: 1, 1, 1, 1
            on_press: root.validate_login()

        Button:
            text: "Go to Sign Up"
            size_hint_y: None
            height: dp(50)
            background_color: 0.29, 0.69, 0.31, 1
            background_normal: ''
            color: 1, 1, 1, 1
            on_press: app.root.current = "signup"

        Button:
            text: "Go Back"
            size_hint_y: None
            height: dp(50)
            background_color: 0.96, 0.26, 0.21, 1
            background_normal: ''
            color: 1, 1, 1, 1
            on_press: app.root.current = "page"

